void rotate(int** arr, int matrixSize, int* Col) {
        for (int i = 0; i < *Col ; i++) {
        for (int j = i; j < *Col; j++) {
            int temp = arr[i][j];
            arr[i][j] = arr[j][i];
            arr[j][i] = temp;
        }
    }
    // Reverse each row
    for (int i = 0; i < *Col; i++) {
        for (int j = 0; j < (*Col)/2; j++) {
            int temp = arr[i][j];
            arr[i][j] = arr[i][*Col-1-j];
            arr[i][*Col-1-j] = temp;
        }
    }
}
